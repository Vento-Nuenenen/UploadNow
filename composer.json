{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "albertcht/invisible-recaptcha": "1.9.6",
        "fideloper/proxy": "4.4.1",
        "fruitcake/laravel-cors": "2.0.4",
        "guzzlehttp/guzzle": "7.3.0",
        "kouz/laravel-airbrake": "0.7.0",
        "laravel/framework": "8.42.1",
        "laravel/tinker": "2.6.1",
        "laravel/ui": "3.2.1",
        "laravelcollective/html": "6.2.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.5.7",
        "barryvdh/laravel-ide-helper": "2.10.0",
        "facade/ignition": "2.9.0",
        "fzaninotto/faker": "1.9.2",
        "mockery/mockery": "1.4.3",
        "nunomaduro/collision": "5.4.0",
        "phpunit/phpunit": "9.5.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
